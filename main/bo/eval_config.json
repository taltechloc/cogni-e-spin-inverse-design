{
  "global": {
    "seed": 42
  },
  "dataset": {
    "file_path": "../data/test.csv",
    "file_type": "csv",
    "target_column": "diameter"
  },
  "model": {
    "type": "XGBoostSurrogate",
    "params": {
      "n_estimators": 100,
      "colsample_bytree": 0.8,
      "learning_rate": 0.05,
      "max_depth": 6,
      "subsample": 0.9,
      "random_state": 42
    }
  },
  "pipeline": {
    "boundaries": {
      "method": "minmax"
    },
    "objective": {
      "type": "SURROGATE",
      "params": {
        "regularization": 0.1
      }
    },
    "optimizer": {
      "n_init": 10,
      "n_iter": 70,
      "acquisition_function": "EI",
      "kappa": 1.5,
      "xi": 0.02,
      "early_stop_patience": 20,
      "optimizer_type": "BO"
    }
  }
}
